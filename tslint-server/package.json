{
  "name": "tslint-server",
  "version": "0.2.0",
  "description": "TSLint Linter Server",
  "engines": {
    "node": "*"
  },
  "private": true,
  "dependencies": {
    "minimatch": "9.0.3",
    "vscode-languageserver": "9.0.1",
    "semver": "7.5.4",
    "vscode-uri": "3.0.8"
  },
  "devDependencies": {
    "@types/minimatch": "5.1.2",
    "@types/mocha": "10.0.3",
    "@types/node": "20.8.9",
    "@types/semver": "7.5.4",
    "mocha": "10.2.0",
    "tslint": "5.20.1",
    "typescript": "5.2.2"
  },
  "scripts": {
    "compile": "installServerIntoExtension ../tslint ./package.json ./src/tsconfig.json && tsc -p ./src",
    "watch": "installServerIntoExtension ../tslint ./package.json ./src/tsconfig.json && tsc --watch -p ./src",
    "pretest": "tsc -p ./test",
    "test": "mocha test/out/test"
  }
}
